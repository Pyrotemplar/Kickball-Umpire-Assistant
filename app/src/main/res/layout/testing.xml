<?xml version="1.0" encoding="utf-8"?>

<com.michaelmuenzer.android.scrollablennumberpicker.ScrollableNumberPicker android:id="@+id/number_picker"
    android:layout_height="wrap_content"
    android:layout_width="wrap_content"
    app:snp_buttonIconLeft="@drawable/ic_remove_circle_outline_black_24dp"
    app:snp_buttonIconRight="@drawable/ic_add_circle_outline_black_24dp"
    app:snp_maxValue="10000"
    app:snp_minValue="10"
    app:snp_orientation="horizontal"
    app:snp_value="10"
    app:snp_valueMarginEnd="8dp"
    app:snp_valueMarginStart="5dp"
    app:snp_value_text_appearance="?android:attr/textAppearanceMedium"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android" />
